:CREATE_AND_POPULATE
   SELECT d.na_feature_id, db_ref_id,
          ':ORG_ABBREV' as org_abbrev, current_timestamp as modification_date
   FROM dots.dbrefnafeature d, dots.nasequence s, dots.nafeature f
   WHERE f.na_sequence_id = s.na_sequence_id
     AND s.taxon_id = :TAXON_ID
     AND d.na_feature_id = f.na_feature_id;
    
:DECLARE_PARTITION;

