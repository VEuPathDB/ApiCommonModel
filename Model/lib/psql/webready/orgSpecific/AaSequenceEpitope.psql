SELECT e.aa_sequence_id, iedb_id, mismatches, protein_match, taxon_match, start_min, end_max, 
       ':ORG_ABBREV' as org_abbrev, current_timestamp as modification_date
 FROM apidb.aasequenceepitope e, dots.aasequence s
 WHERE e.aa_sequence_id = s.aa_sequence_id
   AND s.taxon_id = :TAXON_ID;
    
