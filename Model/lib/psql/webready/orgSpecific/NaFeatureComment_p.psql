:CREATE_AND_POPULATE
   SELECT c.na_feature_id, comment_string, comment_date,
          ':ORG_ABBREV' as org_abbrev, current_timestamp as modification_date
   FROM dots.NaFeatureComment c, dots.nasequence s, dots.nafeature f
   WHERE f.na_sequence_id = s.na_sequence_id
     AND s.taxon_id = :TAXON_ID
     AND c.na_feature_id = f.na_feature_id;
    
:DECLARE_PARTITION;

