:CREATE_AND_POPULATE

      
        CREATE TABLE PathwayNodeGene  as
        SELECT DISTINCT pn.pathway_node_id
          , tp.gene_source_id
        FROM transcriptpathway tp
          , sres.pathwaynode pn
        WHERE tp.pathway_id = pn.pathway_id
          AND tp.ec_number_gene like replace(pn.display_label, '-', '%')
      
    
:DECLARE_PARTITION;

