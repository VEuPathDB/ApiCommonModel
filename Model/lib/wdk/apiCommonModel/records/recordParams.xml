  <querySet name="RecordVQ" queryType="vocab" isCacheable="true">
    <sqlQuery name="Organism">
      <column name="internal" />
      <column name="term" />
      <sql>
	<![CDATA[
	-- PlasmoDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'PlasmoDB'
	UNION

	-- GiardiaDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'GiardiaDB'
	UNION

	
	-- AmoebaDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'AmoebaDB'
	UNION
	
	-- MicrosporidiaDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'MicrosporidiaDB'
	UNION

	
	-- PiroplasmaDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'PiroplasmaDB'
	UNION


	-- CryptoDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'CryptoDB'
	UNION



	-- ToxoDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'ToxoDB'
	UNION

	-- TriTrypDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'TriTrypDB'
	UNION

	-- FungiDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'FungiDB'
	UNION

	-- TrichDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'TrichDB'

	UNION

	-- HostDB
        select distinct organism as term, organism as internal from apidbtuning.geneattributes where project_id = 'HostDB'

	]]>
      </sql>
    </sqlQuery>


    <sqlQuery name="Species">
